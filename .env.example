# Database Configuration
DATABASE_PROVIDER=sqlite
# Options: sqlite, postgresql, mysql
# For PostgreSQL: DATABASE_PROVIDER=postgresql
# For MySQL: DATABASE_PROVIDER=mysql

DATABASE_URL=./dev.db
# For PostgreSQL: DATABASE_URL=postgresql://user:password@localhost:5432/dbname
# For MySQL: DATABASE_URL=mysql://user:password@localhost:3306/dbname

# Application
NODE_ENV=development

# Next.js
NEXT_PUBLIC_APP_URL=http://localhost:3000

